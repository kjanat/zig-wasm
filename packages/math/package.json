{
  "name": "@zig-wasm/math",
  "version": "0.1.0",
  "description": "High-performance math functions powered by Zig via WebAssembly",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "math",
    "trigonometry",
    "sqrt",
    "pow",
    "log",
    "exp"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/zig-wasm.git",
    "directory": "packages/math"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./math.wasm": "./dist/math.wasm"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zig-wasm/core": "workspace:*"
  }
}
