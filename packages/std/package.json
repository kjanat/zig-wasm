{
  "name": "@zig-wasm/std",
  "version": "0.1.0",
  "description": "Zig standard library for JavaScript via WebAssembly - umbrella package",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "std",
    "stdlib"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./base64": {
      "require": "./dist/base64.cjs",
      "import": "./dist/base64.mjs"
    },
    "./compress": {
      "require": "./dist/compress.cjs",
      "import": "./dist/compress.mjs"
    },
    "./crypto": {
      "require": "./dist/crypto.cjs",
      "import": "./dist/crypto.mjs"
    },
    "./hash": {
      "require": "./dist/hash.cjs",
      "import": "./dist/hash.mjs"
    },
    "./math": {
      "require": "./dist/math.cjs",
      "import": "./dist/math.mjs"
    },
    "./package.json": "./package.json",
    "./crypto.wasm": "@zig-wasm/crypto/crypto.wasm",
    "./hash.wasm": "@zig-wasm/hash/hash.wasm",
    "./base64.wasm": "@zig-wasm/base64/base64.wasm",
    "./math.wasm": "@zig-wasm/math/math.wasm",
    "./compress.wasm": "@zig-wasm/compress/compress.wasm"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zig-wasm/base64": "workspace:*",
    "@zig-wasm/compress": "workspace:*",
    "@zig-wasm/core": "workspace:*",
    "@zig-wasm/crypto": "workspace:*",
    "@zig-wasm/hash": "workspace:*",
    "@zig-wasm/math": "workspace:*"
  }
}
