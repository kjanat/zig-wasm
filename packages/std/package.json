{
  "name": "@zig-wasm/std",
  "version": "0.1.0",
  "description": "Zig standard library for JavaScript via WebAssembly - umbrella package",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "std",
    "stdlib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/zig-wasm.git",
    "directory": "packages/std"
  },
  "license": "MIT",
  "author": "Kaj Kowalski <dev@kajkowalski.nl> (https://github.com/kjanat)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./base64": {
      "require": "./dist/base64.cjs",
      "import": "./dist/base64.mjs"
    },
    "./compress": {
      "require": "./dist/compress.cjs",
      "import": "./dist/compress.mjs"
    },
    "./crypto": {
      "require": "./dist/crypto.cjs",
      "import": "./dist/crypto.mjs"
    },
    "./hash": {
      "require": "./dist/hash.cjs",
      "import": "./dist/hash.mjs"
    },
    "./math": {
      "require": "./dist/math.cjs",
      "import": "./dist/math.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "fmt": "dprint fmt",
    "lint": "biome lint",
    "lint:fix": "biome check --write && pnpm fmt",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@zig-wasm/base64": "workspace:*",
    "@zig-wasm/compress": "workspace:*",
    "@zig-wasm/core": "workspace:*",
    "@zig-wasm/crypto": "workspace:*",
    "@zig-wasm/hash": "workspace:*",
    "@zig-wasm/math": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@zig-wasm/tooling": "workspace:*",
    "dprint": "catalog:"
  }
}
