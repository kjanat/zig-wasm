{
  "name": "@zig-wasm/compress",
  "version": "0.1.0",
  "description": "XZ and LZMA decompression powered by Zig's std.compress via WebAssembly",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "compress",
    "decompress",
    "xz",
    "lzma"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/zig-wasm.git",
    "directory": "packages/compress"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./compress.wasm": "./dist/compress.wasm"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zig-wasm/core": "workspace:*"
  }
}
