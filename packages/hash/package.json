{
  "name": "@zig-wasm/hash",
  "version": "0.1.0",
  "description": "Fast non-cryptographic hash functions powered by Zig via WebAssembly",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "hash",
    "crc32",
    "xxhash",
    "wyhash",
    "cityhash",
    "murmur",
    "fnv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/zig-wasm.git",
    "directory": "packages/hash"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./hash.wasm": "./dist/hash.wasm"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@zig-wasm/core": "workspace:*"
  }
}
