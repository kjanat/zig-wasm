{
  "name": "@zig-wasm/hash",
  "version": "0.1.0",
  "description": "Fast non-cryptographic hash functions powered by Zig via WebAssembly",
  "keywords": [
    "zig",
    "wasm",
    "webassembly",
    "hash",
    "crc32",
    "xxhash",
    "wyhash",
    "cityhash",
    "murmur",
    "fnv"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/zig-wasm.git",
    "directory": "packages/hash"
  },
  "license": "MIT",
  "author": "Kaj Kowalski <dev@kajkowalski.nl> (https://github.com/kjanat)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json",
    "./hash.wasm": "./wasm/hash.wasm"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "fmt": "dprint fmt",
    "lint": "biome lint",
    "lint:fix": "biome check --write && pnpm fmt",
    "test": "vitest run",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@zig-wasm/core": "workspace:*"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@typescript/native-preview": "catalog:",
    "@zig-wasm/tooling": "workspace:*",
    "dprint": "catalog:"
  }
}
